<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zyh.code.mapper.AccountMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.zyh.code.entity.Account">
                    <id column="user_id" property="userId" />
                    <result column="total" property="total" />
                    <result column="used" property="used" />
                    <result column="residue" property="residue" />
                    <result column="create_time" property="createTime" />
                    <result column="update_time" property="updateTime" />
        </resultMap>
        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
                user_id, total, used, residue, create_time, update_time
        </sql>

    <select id="selectAccountList" resultType="com.zyh.code.support.vo.AccountVO">
        select
        <include refid="com.zyh.code.mapper.AccountMapper.Base_Column_List"></include>
        from t_account
        order by create_time desc limit 10;
    </select>
</mapper>
